{
  "name": "fibo.github.io",
  "version": "1.0.0",
  "description": "My digital copybook",
  "private": true,
  "scripts": {
    "browserify": "npm run browserify:js:blog",
    "browserify:js:blog": "browserify -e src/blog.js -o js/blog.js",
    "browserify:TODO": "minification transform",
    "cp": "npm run cp:flow; npm run cp:metadata",
    "cp:flow": "cp src/flow/*json flow/",
    "cp:metadata": "npm run cp:metadata:bower.json; npm run cp:metadata:package.json",
    "cp:metadata:bower.json": "cp bower.json _data/",
    "cp:metadata:package.json": "cp package.json _data/",
    "jshint": "jshint -c .jshintrc src/*.js",
    "project:gethomepage": "npm run project:gethomepage:dotsoftware; npm run project:gethomepage:gh-clone; npm run project:gethomepage:nz-util",
    "project:gethomepage:dosoftware": "wget https://raw.githubusercontent.com/fibo/.software/master/README.md -O _projects/_readmes/dotsoftware-readme.md",
    "project:gethomepage:gh-clone": "wget https://raw.githubusercontent.com/fibo/gh-clone/master/README.md -O _projects/_readmes/gh-clone-readme.md",
    "project:gethomepage:nz-util": "wget https://raw.githubusercontent.com/fibo/nz-util/master/README.md -O _projects/_readmes/nz-util-readme.md",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fibo/fibo.github.io.git"
  },
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": [
    {
      "type": "MIT",
      "url": "http://g14n.info/mit-license"
    }
  ],
  "bugs": {
    "url": "https://github.com/fibo/fibo.github.io/issues"
  },
  "homepage": "http://g14n.info",
  "dependencies": {
    "browserify": "^10.2.3",
    "dflow": "^0.4.0"
  }
}
